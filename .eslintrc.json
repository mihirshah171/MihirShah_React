{
    "env": {
        "browser": true,
        "es2020": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "settings": {
        "react": {
            "version": "16.8.2"
        }
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "eslint:all"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": "off",
        "react/prop-types": "off",
        "max-lines-per-function": "off",
        "no-underscore-dangle": "off",
        "require-unicode-regexp": "off",
        "no-ternary": "off",
        "max-len": "off",
        "multiline-ternary": "off",
        "no-magic-numbers": "off",
        "complexity": "off",
        "class-methods-use-this": "off",
        "linebreak-style": "off",
        "indent": "off",
        "space-before-function-paren": "off",
        "no-invalid-this": "off",
        "implicit-arrow-linebreak": "off",
        "comma-dangle": "off",
        "no-trailing-spaces": "off",
        "max-params": "off"
    }
}